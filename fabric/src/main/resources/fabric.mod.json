{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${version}",
  "name": "${modName}",
  "icon": "${modIcon}",
  "description": "${modDescription}",
  "authors": ["${modAuthorsInline}"],
  "license": "${modLicense}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "my.datsuns.fancyfisher.FancyFisherFabric"
    ]
  },
  "mixins": [
    "fancyfisher.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${minecraft_version}",
    "fabric-api": "*"
  }
}
